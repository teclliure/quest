{% extends '::skeleton.html.twig' %}

{% block breadrumb %}
    <div id="breadcrumb" class="span12">
        <ul class="breadcrumb boxed">
            <li><a href="">Bread1</a></li>
            <li><a href="">Bread2</a></li>
        </ul>
    </div>
{% endblock %}

{% block content %}
<div id="navigation" class="span3">
    {% block left_content %}
        <div class="well">
            {% block navigation %}
                <h2>Navigation</h2>
                {#{{ knp_menu_render('main') }}#}
            {% endblock %}
        </div>
    {% endblock %}
</div>
<div class="span9">
    <div id="content">
        <div class="inner">
            {% block inner_content %}
            {% endblock %}
        </div>
    </div>
</div>
{% endblock %}
