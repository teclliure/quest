{% extends '::one_column.html.twig' %}

{% block title %}{{ "Questionary Creation" | trans }}{% endblock %}

{% block inner_content %}
<form action="{{ path('questionary_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p>
        <button class="btn btn-primary" type="submit">Create</button>
    </p>
</form>

<div class="btn-group">
    <a class="btn btn-mini" href="{{ path('questionary') }}">
        Back to the list
    </a>
</div>
{% endblock %}